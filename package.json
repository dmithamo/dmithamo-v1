    "lint-staged": "^10.4.0",
    "prettier": "2.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint:all": "eslint --fix \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmithamo/dmithamo-v1"
  },
  "bugs": {
    "url": "https://github.com/dmithamo/dmithamo-v1/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,md}": [
      "prettier --write",
      "npm run lint:all"
    ]
  }
